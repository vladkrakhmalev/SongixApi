{
  "name": "songix-back",
  "type": "module",
  "scripts": {
    "dev": "NODE_NO_WARNINGS=1 nodemon --exec node --loader ts-node/esm src/server.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "migrate": "sequelize-cli db:migrate",
